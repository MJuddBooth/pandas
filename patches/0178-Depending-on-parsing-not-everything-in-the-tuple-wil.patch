From 8b56d0537a0e2fc93703c121f978ee331672360e Mon Sep 17 00:00:00 2001
From: Michael Booth <m.judd.booth@gmail.com>
Date: Thu, 31 Jul 2014 21:33:58 -0400
Subject: [PATCH 0178/1009] Depending on parsing, not everything in the tuple
 will be a string.  So cast accordingly.

---
 pandas/io/parsers.py | 8 +++++---
 1 file changed, 5 insertions(+), 3 deletions(-)

diff --git a/pandas/io/parsers.py b/pandas/io/parsers.py
index bd53caf..abbddaa 100644
--- a/pandas/io/parsers.py
+++ b/pandas/io/parsers.py
@@ -819,7 +819,7 @@ class ParserBase(object):
         # if we find 'Unnamed' all of a single level, then our header was too
         # long
         for n in range(len(columns[0])):
-            if all(['Unnamed' in c[n] for c in columns]):
+            if all(['Unnamed' in str(c[n]) for c in columns]):
                 raise _parser.CParserError(
                     "Passed header=[%s] are too many rows for this "
                     "multi_index of columns"
@@ -1563,10 +1563,12 @@ class PythonParser(ParserBase):
         if self.header is not None:
             header = self.header
 
-            # we have a mi columns, so read and extra line
+            # we have a mi columns, so read and extra line - Why, asks MJB?
+            
             if isinstance(header, (list, tuple, np.ndarray)):
                 have_mi_columns = True
-                header = list(header) + [header[-1] + 1]
+                #header = list(header) + [header[-1] + 1] # don't assume list is ordered
+                header  = list(header) + [max(list(header)) + 1]
             else:
                 have_mi_columns = False
                 header = [header]
